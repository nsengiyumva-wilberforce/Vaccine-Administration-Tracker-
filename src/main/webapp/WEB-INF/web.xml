<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>healthcentresadministration</servlet-name>
        <jsp-file>/healthcentresadministration.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>systemsadministration</servlet-name>
        <jsp-file>/systemsadministration.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>booking</servlet-name>
        <jsp-file>/booking.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>vaccineadministration</servlet-name>
        <jsp-file>/vaccineadministration.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>vaccineinventory</servlet-name>
        <jsp-file>/vaccineinventory.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>org.authentication.Signup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>healthcentresadministration</servlet-name>
        <url-pattern>/healthcentres</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>systemsadministration</servlet-name>
        <url-pattern>/systemsadministration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>booking</servlet-name>
        <url-pattern>/booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vaccineadministration</servlet-name>
        <url-pattern>/vaccineadministration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>vaccineinventory</servlet-name>
        <url-pattern>/vaccineinventory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
</web-app>
